<t:templates xmlns:t="http://nwalsh.com/docbook/xsl/template/1.0"
  xmlns:param="http://nwalsh.com/docbook/xsl/template/1.0/param" xmlns="http://www.w3.org/1999/xhtml"
  xmlns:xsl="http://www.w3.org/1999/XSL/Transform">

  <t:titlepage t:element="book" t:wrapper="div" class="titlepage">
    <t:titlepage-content t:side="recto">
      <title />
      <subtitle />
      <isbn />
      <issn />
      <biblioid />
      <corpauthor />
      <authorgroup />
      <author />
      <othercredit />
      <releaseinfo />
      <copyright />
      <legalnotice />
      <pubdate />
      <printhistory />
      <revision />
      <revhistory />
      <abstract />
    </t:titlepage-content>

    <t:titlepage-content t:side="verso">
    </t:titlepage-content>

    <t:titlepage-separator>
      <hr />
    </t:titlepage-separator>

    <t:titlepage-before t:side="recto">
    </t:titlepage-before>

    <t:titlepage-before t:side="verso">
    </t:titlepage-before>
  </t:titlepage>

  <t:titlepage t:element="statblocksection" t:wrapper="div" class="titlepage">
    <t:titlepage-content t:side="recto">
      <title />
    </t:titlepage-content>

    <t:titlepage-content t:side="verso">
    </t:titlepage-content>

    <t:titlepage-separator>
    </t:titlepage-separator>

    <t:titlepage-before t:side="recto">
    </t:titlepage-before>

    <t:titlepage-before t:side="verso">
    </t:titlepage-before>
  </t:titlepage>
</t:templates>